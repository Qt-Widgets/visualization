<?xml version="1.0" encoding="utf-8"?>
<visu_config>
   <configuration>
       <port>3334</port>
       <width>900</width>
       <height>240</height>
       <color_background>30,30,30,255</color_background>
       <name>CPU monitor</name>
   </configuration>
   <signals>
       <signal>
           <id>1</id>
           <name>CPU</name>
           <unit>%</unit>
           <factor>1.0</factor>
           <offset>0.0</offset>
           <min>0</min>
           <max>100</max>
       </signal>
       <signal>
           <id>2</id>
           <name>RAM</name>
           <unit>MB</unit>
           <factor>1000.0</factor>
           <offset>0.0</offset>
           <min>0</min>
           <max>16</max>
       </signal>
   </signals>
     <instruments>
       <instrument>
           <id>1</id>
           <type>ANALOG</type>
           <signal_id>1</signal_id>
           <name>Analog 1</name>
           <width>300</width>
           <height>300</height>
           <x>30</x>
           <y>20</y>
           <color_background>30,30,30,255</color_background>
           <color_foreground>250,50,50,100</color_foreground>
           <color_static>255,50,50,255</color_static>
           <line_thickness>5</line_thickness>
           <major_len>10</major_len>
           <minor_len>5</minor_len>
           <major_cnt>5</major_cnt>
           <minor_cnt>2</minor_cnt>
           <font_size>14</font_size>
           <angle_start>1.570795</angle_start>
           <angle_end>3.14159</angle_end>
           <arrow_width>5</arrow_width>
           <draw_circle>0</draw_circle>
           <label_radius>95</label_radius>
           <name_x>120</name_x>
           <name_y>200</name_y>
           <offset_x>0</offset_x>
           <offset_y>0</offset_y>
       </instrument>
       <instrument>
           <id>6</id>
           <type>TIME_PLOT</type>
           <signal_id>1</signal_id>
           <name>Time plot</name>
           <width>650</width>
           <height>180</height>
           <x>220</x>
           <y>10</y>
           <color_background>30,30,30,255</color_background>
           <color_foreground>255,100,100,250</color_foreground>
           <color_static>200,0,0,255</color_static>
           <color_grid>50,50,50,255</color_grid>
           <line_thickness>2</line_thickness>
           <major_len>10</major_len>
           <minor_len>4</minor_len>
           <major_cnt>10</major_cnt>
           <minor_cnt>2</minor_cnt>
           <font_size>10</font_size>
           <arrow_width>3</arrow_width>
           <draw_circle>30</draw_circle>
           <label_radius>20</label_radius>
           <name_x>40</name_x>
           <name_y>100</name_y>
           <offset_x>-40</offset_x>
           <offset_y>0</offset_y>
           <ticks_in_second>1000</ticks_in_second>
           <timespan>30000</timespan>
           <marker_dt>2000</marker_dt>
           <decimals>1</decimals>
           <division_format>mm:ss</division_format>
           <master_time_format>hh:mm:ss</master_time_format>
       </instrument>
      <instrument>
           <id>6</id>
           <type>DIGITAL</type>
           <signal_id>1</signal_id>
           <name>Digital</name>
           <width>80</width>
           <height>30</height>
           <x>120</x>
           <y>120</y>
           <color_background>30,30,30,255</color_background>
           <color_foreground>255,100,100,250</color_foreground>
           <color_static>200,0,0,255</color_static>
           <color_grid>50,50,50,255</color_grid>
           <line_thickness>2</line_thickness>
           <major_len>10</major_len>
           <minor_len>4</minor_len>
           <major_cnt>10</major_cnt>
           <minor_cnt>2</minor_cnt>
           <font_size>25</font_size>
           <arrow_width>3</arrow_width>
           <draw_circle>30</draw_circle>
           <label_radius>20</label_radius>
           <name_x>40</name_x>
           <name_y>100</name_y>
           <offset_x>-40</offset_x>
           <offset_y>0</offset_y>
           <show_signal_unit>1</show_signal_unit>
           <show_signal_name>0</show_signal_name>
           <padding>0</padding>
       </instrument>
       <instrument>
            <id>4</id>  
            <type>LED</type>
            <signal_id>1</signal_id>
            <name>Led</name>
            <width>100</width>
            <height>30</height>
            <x>10</x>
            <y>10</y>
            <color_background>30,30,30,255</color_background>
            <color_foreground>255,100,100,250</color_foreground>
            <color_static>200,0,0,255</color_static>
            <color_on>30,255,30,255</color_on>
            <color_off>255,30,30,250</color_off>
            <radius>25</radius>
            <val1>-10.0</val1>
            <val2>6.0</val2>
            <condition>0</condition>
            <show_signal_lbl>1</show_signal_lbl>
            <font_size>26</font_size>
       </instrument>
   </instruments>
</visu_config>

