<?xml version="1.0" encoding="utf-8"?>
<visu_config>
   <configuration>
       <port>3334</port>
       <width>900</width>
       <height>240</height>
       <colorBackground>30,30,30,255</colorBackground>
       <name>CPU monitor</name>
   </configuration>
   <signals>
       <signal>
           <id>1</id>
           <name>CPU</name>
           <unit>%</unit>
           <factor>1.0</factor>
           <offset>0.0</offset>
           <min>0</min>
           <max>100</max>
       </signal>
       <signal>
           <id>2</id>
           <name>RAM</name>
           <unit>MB</unit>
           <factor>1000.0</factor>
           <offset>0.0</offset>
           <min>0</min>
           <max>16</max>
       </signal>
   </signals>
     <instruments>
       <instrument>
           <id>1</id>
           <type>ANALOG</type>
           <signalId>1</signalId>
           <name>Analog 1</name>
           <width>300</width>
           <height>300</height>
           <x>30</x>
           <y>20</y>
           <colorBackground>30,30,30,255</colorBackground>
           <colorForeground>250,50,50,100</colorForeground>
           <colorStatic>255,50,50,255</colorStatic>
           <lineThickness>5</lineThickness>
           <majorLen>10</majorLen>
           <minorLen>5</minorLen>
           <majorCnt>5</majorCnt>
           <minorCnt>2</minorCnt>
           <fontSize>14</fontSize>
           <angleStart>1.570795</angleStart>
           <angleEnd>3.14159</angleEnd>
           <arrowWidth>5</arrowWidth>
           <drawCircle>0</drawCircle>
           <labelRadius>95</labelRadius>
           <nameX>120</nameX>
           <nameY>200</nameY>
           <offsetX>0</offsetX>
           <offsetY>0</offsetY>
       </instrument>
       <instrument>
           <id>6</id>
           <type>TIME_PLOT</type>
           <signalId>1</signalId>
           <name>Time plot</name>
           <width>650</width>
           <height>180</height>
           <x>220</x>
           <y>10</y>
           <colorBackground>30,30,30,255</colorBackground>
           <colorForeground>255,100,100,250</colorForeground>
           <colorStatic>200,0,0,255</colorStatic>
           <lineThickness>2</lineThickness>
           <majorLen>10</majorLen>
           <minorLen>4</minorLen>
           <majorCnt>10</majorCnt>
           <minorCnt>2</minorCnt>
           <fontSize>10</fontSize>
           <arrowWidth>3</arrowWidth>
           <drawCircle>30</drawCircle>
           <labelRadius>20</labelRadius>
           <nameX>40</nameX>
           <nameY>100</nameY>
           <offsetX>-40</offsetX>
           <offsetY>0</offsetY>
           <ticksInSecond>100</ticksInSecond>
           <timespan>3000</timespan>
           <markerDt>500</markerDt>
           <decimals>1</decimals>
           <divisionFormat>mm:ss</divisionFormat>
           <masterTimeFormat>hh:mm:ss</masterTimeFormat>
       </instrument>
      <instrument>
           <id>6</id>
           <type>DIGITAL</type>
           <signalId>1</signalId>
           <name>Digital</name>
           <width>80</width>
           <height>30</height>
           <x>120</x>
           <y>120</y>
           <colorBackground>30,30,30,255</colorBackground>
           <colorForeground>255,100,100,250</colorForeground>
           <colorStatic>200,0,0,255</colorStatic>
           <color_grid>50,50,50,255</color_grid>
           <lineThickness>2</lineThickness>
           <majorLen>10</majorLen>
           <minorLen>4</minorLen>
           <majorCnt>10</majorCnt>
           <minorCnt>2</minorCnt>
           <fontSize>25</fontSize>
           <arrowWidth>3</arrowWidth>
           <drawCircle>30</drawCircle>
           <labelRadius>20</labelRadius>
           <nameX>40</nameX>
           <nameY>100</nameY>
           <offsetX>-40</offsetX>
           <offsetY>0</offsetY>
           <showSignalUnit>1</showSignalUnit>
           <showSignalName>0</showSignalName>
           <padding>0</padding>
       </instrument>
       <instrument>
            <id>4</id>  
            <type>LED</type>
            <signalId>1</signalId>
            <name>Led</name>
            <width>100</width>
            <height>30</height>
            <x>10</x>
            <y>10</y>
            <colorBackground>30,30,30,255</colorBackground>
            <colorForeground>255,100,100,250</colorForeground>
            <colorStatic>200,0,0,255</colorStatic>
            <colorOn>30,255,30,255</colorOn>
            <colorOff>255,30,30,250</colorOff>
            <radius>25</radius>
            <val1>-10.0</val1>
            <val2>6.0</val2>
            <condition>0</condition>
            <showSignalName>1</showSignalName>
            <fontSize>26</fontSize>
       </instrument>
   </instruments>
</visu_config>

